{
  "name": "data-channels-builtin-processors",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "build": "mkdirp dist && npm run compile",
    "lint": "tslint --fix --project tsconfig.json",
    "clean": "shx rm -rf dist coverage",
    "compile": "tsc",
    "compile:once": "tsc --noEmit",
    "compile:watch": "tsc --watch --noEmit",
    "test": "npm run lint && npm run compile:once && npm run test:cov",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "test:nocov": "jest --no-cache --runInBand",
    "sam-package-sandbox": "sam package --output-template-file packaged-sandbox.yml --s3-bucket data-channels-processor-deployments-sandbox --profile data-channels-sandbox",
    "sam-deploy-sandbox": "sam deploy --template-file ./packaged-sandbox.yml --stack-name data-channels-builtin-processor --capabilities CAPABILITY_IAM --region us-east-1 --profile data-channels-sandbox",
    "deploy-sandbox": "npm run build && sam build && npm run sam-package-sandbox && npm run sam-deploy-sandbox",
    "sam-package-dev": "sam package --output-template-file packaged-dev.yml --s3-bucket data-channels-processor-deployments-dev --profile data-channels-dev",
    "sam-deploy-dev": "sam deploy --template-file ./packaged-dev.yml --stack-name data-channels-builtin-processor-dev --capabilities CAPABILITY_IAM --region us-east-1",
    "deploy-dev": "npm run build && sam build && npm run sam-package-dev && npm run sam-deploy-dev",
    "sam-package-navprod": "sam package --output-template-file packaged-navprod.yml --s3-bucket data-channels-processor-deployments-navprod --profile data-channels-naviance-prod",
    "sam-deploy-navprod": "sam deploy --template-file ./packaged-navprod.yml --stack-name data-channels-builtin-processor-navprod --capabilities CAPABILITY_IAM --region us-east-1",
    "deploy-navprod": "npm run build && sam build && npm run sam-package-navprod && npm run sam-deploy-navprod",
    "deploy": "node deploy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hobsons/data-channels-processors.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hobsons/data-channels-processors/issues"
  },
  "homepage": "https://github.com/Hobsons/data-channels-processors#readme",
  "devDependencies": {
    "@types/adm-zip": "0.4.33",
    "@types/aws-lambda": "8.10.31",
    "@types/jest": "24.0.11",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.117",
    "@types/node": "13.13.5",
    "@types/node-fetch": "2.5.7",
    "@types/uuid": "7.0.3",
    "aws-sdk": "2.492.0",
    "aws-sdk-mock": "5.1.0",
    "jest": "24.4.0",
    "mkdirp": "0.5.1",
    "shx": "0.3.2",
    "ts-jest": "24.0.0",
    "ts-node": "8.10.1",
    "tslint": "5.11.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@data-channels/dcSDK": "0.20.2",
    "csv": "5.3.2",
    "lodash": "4.17.15",
    "node-fetch": "2.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!<rootDir>/node_modules/",
      "!src/**/__*__/*.ts",
      "!src/types/*.d.ts",
      "!src/index.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "src"
    ],
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true
  }
}
